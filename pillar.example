logstash_forwarder:
        servers: 
            - logs.example.com:5000
        files:
            -
                paths:
                    - /var/log/syslog
                    - /var/log/auth.log
                fields:
                    type: syslog
            -
                paths:
                    - /var/log/nginx/*access*
                fields:
                    type: nginx-access
            -
                paths:
                    - /var/log/nginx/*error*
                fields:
                    type: nginx-error
        # Extend the 'files' option. Can be specified in multiple locations/pillar files.
        files_extend:
            apache:
                paths:
                    - /var/log/apache2/*.log
                fields:
                    type: apache
        cert_path: /etc/ssl/certs/logstash-forwarder.crt
        log_to_syslog: true
        quiet: true
